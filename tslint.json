{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "prefer-const": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "indent": true,
        "deprecation": true,
        "completed-docs": true,
        "trailing-comma": true,
        "no-any": true,
        "semicolon": true,
    },
    "rulesDirectory": []
}